{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false
  },
"include": [
  "next-env.d.ts",
  "**/*.ts",
  "**/*.tsx",
  "**/*.js",
  "**/*.jsx",
  ".next/types/**/*.ts"
],
  "exclude": ["node_modules"]
}
